<% provide(:title, "Sign up") %>

<%= form_for @user do |f| %>
  <%= f.label :name, "フルネーム" %>
  <%= f.text_field :name %>

  <%= f.label :user_name %>
  <%= f.text_field :user_name %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation, "確認用" %>
  <%= f.password_field :password_confirmation %>

  

  <%= f.submit "登録する" %>

<% end %>
